        <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="BeanShell PreProcessor" enabled="true">
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="script">import java.time.LocalDate;
import java.util.Random;
  
            Random random = new Random();
            int one = random.nextInt(30);
            int two = random.nextInt(30);
            LocalDate arrivalDate = LocalDate.now().plusDays(1);
            LocalDate departureDate = arrivalDate.plusDays(1);
            if(one &lt; two) {
                arrivalDate = arrivalDate.plusDays(one);
                departureDate = departureDate.plusDays(two);
            } else if(two &lt; one) {
                arrivalDate = arrivalDate.plusDays(two);
                departureDate = departureDate.plusDays(one);
            } else {
                arrivalDate = arrivalDate.plusDays(one);
                departureDate = departureDate.plusDays(two + 1);
            }
 vars.put(&quot;arrivalDate&quot;, arrivalDate.toString());	      
 vars.put(&quot;departureDate&quot;, departureDate.toString());	      </stringProp>
        </BeanShellPreProcessor>
